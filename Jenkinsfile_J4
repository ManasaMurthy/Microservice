#!groovy

node {
checkout scm
env.WORKSPACE = pwd()
def file = new File("${env.WORKSPACE}/Book1.csv")
def lines = file.readLines()
        lines.each { String line ->
            def values = line.split(',')
            build job: 'J3', parameters: [[$class: 'StringParameterValue', name: 'REPO_NAME', value: values[0]],[$class: 'StringParameterValue', name: 'BRANCH_NAME', value: values[1]],[$class: 'StringParameterValue', name: 'PROJECT_TYPE', value: values[2]]]
        }

}
 
