#!groovy
node {
cleanWs() 
sh ("wget '${env.REPO_FILE_URL}'")
def file = new File("${env.WORKSPACE}/${env.REPO_FILE_NAME}")
def lines = file.readLines()
        lines.each { String line ->
            def values = line.split(',')
            
                build job: 'J1', parameters: [[$class: 'StringParameterValue', name: 'ORG_NAME',value: env.ORG_NAME],[$class: 'StringParameterValue', name: 'REPO_NAME', value: values[0]],[$class: 'StringParameterValue', name: 'TOKEN', value: env.TOKEN],[$class: 'StringParameterValue', name: 'PROJECT_TYPE', value: values[1]]]
      }

}
